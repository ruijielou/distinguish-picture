/**index.wxss**/
.container {
  width: 100%;
  height: 100%;
  background-color: green;
  background-image: url("data:image/png;base64,");
  background-position: center;
  background-size: 100px 100px;
  background-repeat: no-repeat;
}
.image {
  height: 50%;
  width: 90%;
  padding: 50rpx;
  position: relative;
  left: 5%;
  box-sizing: border-box;
  background: rgba(130, 121, 121, 0.56);
  transform: translateY(50%);
}
.image image {
  width: 100%;
  height: 100%;
}
.close-icon {
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  width: 40rpx;
  height: 40rpx;
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
}

.result::before {
  position: absolute;
  bottom: -74rpx;
  left: 50%;
  transform: translateX(-50%);
  content: "";
  width: 0;
  height: 0;
  border: 40rpx solid transparent;
  border-top-color: rgb(255, 255, 255);
}

.controls {
  position: fixed;
  bottom: 0;
  margin-bottom: 100rpx;
  width: 100%;
}

.tips {
  position: relative;
  width: 450rpx;
  margin: 50rpx auto;
  padding: 20rpx;
  border-radius: 20rpx;
  /* background: rgba(255, 255, 255, 0.8); */
  text-align: center;
  font-size: 24rpx;
  animation: tips 0.8s infinite;
}

.tips::after {
  position: absolute;
  bottom: -40rpx;
  left: 225rpx;
  content: "";
  width: 0;
  height: 0;
  border: 20rpx solid transparent;
  border-top-color: rgba(255, 255, 255, 0.8);
}

@keyframes tips {
  from {
    transform: translateY(-30rpx);
  }

  35% {
    transform: translateY(0);
  }

  to {
    transform: translateY(-30rpx);
  }
}

.button {
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 260rpx;
  padding: 15rpx 50rpx;
  border-radius: 50rpx;
  background:white;
  color: #141c1a;
}

.button-hover {
  background: rgba(204, 204, 204, 0.9);
}

.button .icon {
  width: 32rpx;
  height: 32rpx;
  margin: 5rpx;
  background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" viewBox="0 0 24 24"><path d="M23 19a2 2 0 0 1-2 2H3a2 2 0 0 1-2-2V8a2 2 0 0 1 2-2h4l2-3h6l2 3h4a2 2 0 0 1 2 2z"></path><circle cx="12" cy="13" r="4"></circle></svg>');
}

.logs {
  position: fixed;
  top: 90rpx;
  left: 10px;
  z-index: 9;
  width: 62rpx;
  height: 62rpx;
}

.score {
  position: absolute;
  top: 10%;
  left: 50%;
  transform: translateX(-50%);
  background-color: #fff;
  padding: 20rpx;
  border-radius: 5rpx;
}
